{
  "name": "agenda-rest",
  "version": "1.4.0",
  "description": "Scheduling as a Service",
  "main": "./dist/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "build": "cp -R ./src/ ./dist && cp ./test.js ./dist/",
    "start": "npm run build && node cli.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jwalkerlusd/agenda-rest.git"
  },
  "files": [
    "cli.js",
    "settings.js",
    "dist"
  ],
  "keywords": [
    "schedule",
    "scheduling",
    "scheduler",
    "webservice",
    "microservice",
    "restful",
    "rest",
    "agenda"
  ],
  "author": "Keyvan Mir Mohammad Sadeghi <keyvan.m.sadeghi@gmail.com>",
  "license": "MIT",
  "bin": {
    "agenda-rest": "cli.js"
  },
  "bugs": {
    "url": "https://github.com/agenda/agenda-rest/issues"
  },
  "homepage": "https://github.com/agenda/agenda-rest#README",
  "dependencies": {
    "agenda": "^4.0.0",
    "commander": "^8.0.0",
    "koa": "^2.10.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "pythonic": "^2.0.3",
    "request-promise": "^4.2.4"
  },
  "engines": {
    "node": ">=8"
  },
  "xo": {
    "space": 2,
    "rules": {
      "ava/no-inline-assertions": 0,
      "import/namespace": 0,
      "promise/param-names": 0,
      "promise/prefer-await-to-then": 0,
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    }
  }
}
